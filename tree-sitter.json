{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "structured_text",
      "camelcase": "StructuredText",
      "title": "IEC 61131-3 structured text",
      "scope": "source.structured_text",
      "file-types": [
        "st",
        "iec",
        "scl"
      ],
      "injection-regex": "^structured_text$",
      "class-name": "TreeSitterStructuredText"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "IEC 61131-3 structured text",
    "authors": [
      {
        "name": "Teun Reyniers",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/teunreyniers/tree-sitter-structured-text",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}